<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/feedback_highlight_solid" />
            <stroke android:width="@dimen/feedback_color_selection_indicator_width" android:color="@color/feedback_color_selection_indicator" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/feedback_highlight_solid" />
        </shape>
    </item>
</selector>